{
    "required_api_version": "^2.0.0",
    "name": "ChatGPT",
    "description": "Create a domain specific assistant using GPT-3.5",
    "developer_name": "SeoFernando",
    "icon": "images/icon.png",
    "options": {
        "query_debounce": 2
    },
    "preferences": [
        {
            "id": "gpt_kw",
            "type": "keyword",
            "name": "GPT",
            "default_value": "gpt"
        },
        {
            "id": "api_key",
            "type": "input",
            "name": "OpenAI API Key",
            "description": "You can get one at https://platform.openai.com/account/api-keys",
            "default_value": ""
        },
        {
            "id": "api_endpoint",
            "type": "input",
            "name": "OpenAI Endpoint",
            "description": "Something like https://api.openai.com/v1/chat/completions",
            "default_value": ""
        }
    ]
}
